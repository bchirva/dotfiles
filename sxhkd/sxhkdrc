# terminal emulator
super + Return
    $TERMINAL 

# launcher
super + space
    rofi -modi drun -show drun

alt + Tab
    rofi -modi window -show window

super + c
    rofi-master

super + Print
    rofi-screen-capture

super + p
    rofi-passwords

super + m
    rofi-music

super + w
    $BROWSER

